<UserControl x:Class="NodeModelsEssentials.Controls.StateDynamoControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NodeModelsEssentials.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Label Content="{Binding State}" HorizontalAlignment="Left" Margin="54,10,0,0" VerticalAlignment="Top"/>
        <Label Content="{Binding Executions}" HorizontalAlignment="Left" Margin="54,35,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
